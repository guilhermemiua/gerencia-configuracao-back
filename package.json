{
  "name": "gerencia-configuracao-back",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/guilhermemiua/gerencia-configuracao-back.git",
  "author": "Guilherme Eiti Akita Miua <guilhermemiua@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "NODE_ENV=test nodemon src/server.ts",
    "build": "rimraf dist && tsc",
    "migration:make": "NODE_ENV=test knex --knexfile src/database/knexfile.ts -x ts migrate:make",
    "migration:latest": "NODE_ENV=test knex --knexfile src/database/knexfile.ts migrate:latest",
    "migration:rollback": "NODE_ENV=test knex --knexfile src/database/knexfile.ts migrate:rollback",
    "migration:run": "NODE_ENV=test knex --knexfile src/database/knexfile.ts migrate:up",
    "seed:make": "NODE_ENV=test knex --knexfile src/database/knexfile.ts -x ts seed:make",
    "seed:run": "NODE_ENV=test knex --knexfile src/database/knexfile.ts seed:run"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "nodemailer": "^6.4.11",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.9.5",
    "@types/pg": "^7.14.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
